<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.2">
	<process type="import">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
		<source type="xlsfile">
			<!--
			note: all word please use lower-case.
			type="businessobject",dataset,txtfile,xlsfile,database
			-->
			<table type="column" name="cityairport" coltype="name" firstrow="2">
				<!--
				columntype=name, position(column must have col property)
				firstdata=1(only for text source)
				delimiter=tab,comma(only for text file), or none(fix length)
				sql= sql statement (only for database)
				-->

				<field name="destination" caption="目的地" datatype="string" position="1" length="10" pk="1" />
				<field name="province" caption="省份" datatype="string" position="2" length="10" pk="0" />
				<field name="transit" caption="中转城市" datatype="string" position="3" length="10" pk="0" />
			</table>
		</source>
		<!--
	type=xlsfile,txtfile,databases
	method="excel",html,xml
	template's file must store same with this file.
	 -->
		<target seq="1" type="database">
			<table name="impcityairport" type="data">
				<body type="header"></body>
				<body type="body" firstrow="2" maxrecords="-1" sql="stp_imp_cityairport">
					<paramerters>
						<field name="@id" caption="id" datatype="long" length="20" pk="1" value="">
							<data type="runtime" field="id" value="value"></data>
						</field>
					</paramerters>
					<!--rows will repeat output-->
						<cell name="id" col="1" format="text" datatype="long" length="20" pk="1">
							<data type="runtime" field="id" value="value"></data>
						</cell>
						<cell name="type" col="2" format="text" datatype="string" length="3" pk="1">
							<data type="runtime" field="type"  value="value"></data>
						</cell>
						<cell name="destination" col="3" format="text" datatype="string" length="10" pk="1">
							<data type="source" table="cityairport" field="destination" value="value"></data>
						</cell>
						<cell name="province" col="4" format="text" datatype="string" length="10" pk="0">
							<data type="source" table="cityairport" field="province" value="value"></data>
						</cell>
						<cell name="transit" col="5" format="text" datatype="string" length="10" pk="0">
							<data type="source" table="cityairport" field="transit" value="value"></data>
						</cell>

				</body>
				<body type="footer">
					<!--not supported--></body>
			</table>
			<table type="filter" columns="3"></table>
		</target>
	</process>
</define>