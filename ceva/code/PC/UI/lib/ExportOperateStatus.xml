<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.0">
	<process type="export">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
	<source type="DataSet">
		<!--
			note: all word please use lower-case.
			type="businessobject",DataSet,textfile,xlsfile
			object_type: no-use
			columntype=name, position(column must have col property)
			firstdata=1(only for text source)
			delimiter=tab,comma(only for text file)
			-->
							

		<table type="column" name="Operate" coltype="name">
			<field name="Opt_Date" caption="操作日期" datatype="string" length="20" pk="0" />
			<field name="DN" caption="单号" datatype="string" length="20" pk="0" />
			<field name="Sony_Batch" caption="索尼批次" datatype="string" length="20" pk="0" />
			<field name="Ceva_Batch" caption="CEVA批次" datatype="string" length="20" pk="0" />
			<field name="Destination_City" caption="送达城市" datatype="string" length="20" pk="0" />
			<field name="Bin" caption="库位" datatype="string" length="20" pk="0" />
			<field name="Complete_Status" caption="完成状态" datatype="string" length="1" pk="0" />
			<field name="Exception" caption="异常描述" datatype="string" length="20" pk="0" />
		</table>
	</source>
	<!--
	type=xlsfile,textfile
	method="Excel",html,xml
	template's file must store same with this file.
	 -->
	<target seq="1" type="xlsfile" method="Excel">
		<table name="操作状态明细" columns="3" type="data">			
			<body type="header" firstrow="1">
				<cell name="opt_dtime" col="1" format="text">
					<data type="value" value="操作日期"></data>
				</cell>
				<cell name="dn_no" col="2" format="text">
					<data type="value" value="单号"></data>
				</cell>
				<cell name="sony_bch_no" col="3" format="text">
					<data type="value" value="索尼批次"></data>
				</cell>
				<cell name="bch_no" col="4" format="text">
					<data type="value" value="CEVA批次"></data>
				</cell>
				<cell name="city_to" col="5" format="text">
					<data type="value" value="送达城市"></data>
				</cell>
				<cell name="bin_code" col="6" format="text">
					<data type="value" value="库位"></data>
				</cell>
				<cell name="status_code" col="7" format="text">
					<data type="value" value="完成状态"></data>
				</cell>
				<cell name="Exception" col="8" format="text">
					<data type="value" value="异常描述"></data>
				</cell>
			</body>
			<body type="body" firstrow="2" maxrecords="-1">
				<!--Rows will repeat output-->
				<cell name="opt_dtime" col="1" format="text">
					<data type="source" table="Operate" field="opt_dtime" value="value"></data>
				</cell>
				<cell name="dn_no" col="2" format="text">
					<data type="source" table="Operate" field="dn_no" value="value"></data>
				</cell>
				<cell name="sony_bch_no" col="3" format="text">
					<data type="source" table="Operate" field="sony_bch_no" value="value"></data>
				</cell>
				<cell name="bch_no" col="4" format="text">
					<data type="source" table="Performance" field="bch_no" value="value"></data>
				</cell>
				<cell name="city_to" col="5" format="text">
					<data type="source" table="Performance" field="city_to" value="value"></data>
				</cell>
				<cell name="bin_code" col="6" format="text">
					<data type="source" table="Performance" field="bin_code" value="value"></data>
				</cell>
				<cell name="status_code" col="6" format="text">
					<data type="source" table="Performance" field="status_code" value="value"></data>
				</cell>
				<cell name="Exception" col="6" format="text">
					<data type="source" table="Performance" field="Exception" value="value"></data>
				</cell>
			</body>
			<body type="footer"></body>
		</table>
	</target>
	</process>
</define>