<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.2">
	<process type="import">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
		<source type="xlsfile">
			<!--
			note: all word please use lower-case.
			type="businessobject",dataset,txtfile,xlsfile,database
			-->
			<table type="column" name="bin" coltype="name" firstrow="2">
				<!--
				columntype=name, position(column must have col property)
				firstdata=1(only for text source)
				delimiter=tab,comma(only for text file), or none(fix length)
				sql= sql statement (only for database)
				-->

				<field name="bin_code" caption="货位代码" datatype="string" position="1" length="10" pk="1" />
				<field name="length" caption="长" datatype="integer" position="2" length="12" pk="0" />
				<field name="width" caption="宽" datatype="integer" position="3" length="12" pk="0" />
				<field name="weight" caption="高" datatype="integer" position="4" length="12" pk="0" />
				<field name="volume" caption="体积" datatype="integer" position="5" length="12" pk="0" />		
				<!--
				<field name="wh_code" caption="仓库代码" datatype="string" position="4" length="10" pk="0" />
				<field name="description" caption="货位描述" datatype="string" position="" length="50" pk="0" />
				<field name="bin_type" caption="仓库类型" datatype="string" position="5" length="10" pk="0" />
				<field name="bin_area" caption="bin_area" datatype="string" position="6" length="10" pk="0" />
				<field name="area" caption="area" datatype="integer" position="7" length="12" pk="0" />
				<field name="palls" caption="palls" datatype="integer" position="8" length="12" pk="0" />
				<field name="volume" caption="volume" datatype="integer" position="10" length="20" pk="0" />
				<field name="canpick" caption="canpick" datatype="integer" position="11" length="1" pk="0" />
				<field name="isblocked" caption="isblocked" datatype="integer" position="12" length="1" pk="0" />
				<field name="define1" caption="define1" datatype="string" position="13" length="50" pk="0" />
				<field name="define2" caption="define2" datatype="string" position="14" length="50" pk="0" />
				<field name="define3" caption="define3" datatype="string" position="15" length="50" pk="0" />
				<field name="define4" caption="define4" datatype="string" position="16" length="50" pk="0" />
				<field name="define5" caption="define5" datatype="string" position="17" length="50" pk="0" />
				<field name="opt_by" caption="opt_by" datatype="string" position="18" length="20" pk="0" />	
				-->
			</table>
		</source>
		<!--
	type=xlsfile,txtfile,databases
	method="excel",html,xml
	template's file must store same with this file.
	 -->
		<target seq="1" type="database">
			<table name="impbin" type="data">
				<body type="header"></body>
				<body type="body" firstrow="2" maxrecords="-1" sql="stp_imp_Bin">
					<paramerters>
						<field name="@id" caption="id" datatype="long" length="20" pk="1" value="">
							<data type="runtime" field="id" value="value"></data>
						</field>
					</paramerters>
					<!--rows will repeat output-->
						<cell name="id" col="1" format="text" datatype="long" length="20" pk="1">
							<data type="runtime" field="id" value="value"></data>
						</cell>
						<cell name="dc_code" col="2" format="text" datatype="string" length="10" pk="1">
							<data type="value" value="SHA"></data>
						</cell>
						<cell name="bin_code" col="3" format="text" datatype="string" length="10" pk="1">
							<data type="source" table="bin" field="bin_code" value="value"></data>
						</cell>
						<cell name="length" col="4" format="text" datatype="string" length="12" pk="0">
							<data type="source" table="bin" field="length" value="value"></data>
						</cell>
						<cell name="width" col="5" format="text" datatype="string" length="12" pk="0">
							<data type="source" table="bin" field="width" value="value"></data>
						</cell>
						<cell name="weight" col="6" format="text" datatype="string" length="12" pk="0">
							<data type="source" table="bin" field="weight" value="value"></data>
						</cell>
						<cell name="volume" col="7" format="text" datatype="string" length="12" pk="0">
							<data type="source" table="bin" field="volume" value="value"></data>
						</cell>

					<!--
						<cell name="bin_area" col="8" format="text" datatype="string" length="10" pk="0">
							<data type="value" value="A-A"></data>
						</cell>
						<cell name="dc_code" col="2" format="text" datatype="string" length="10" pk="1">
							<data type="source" table="bin" field="dc_code" value="value"></data>
						</cell>
						<cell name="bin_code" col="3" format="text" datatype="string" length="10" pk="1">
							<data type="source" table="bin" field="bin_code" value="value"></data>
						</cell>
						<cell name="description" col="4" format="text" datatype="string" length="50" pk="0">
							<data type="source" table="bin" field="description" value="value"></data>
						</cell>
						<cell name="wh_code" col="5" format="text" datatype="string" length="10" pk="0">
							<data type="source" table="bin" field="wh_code" value="value"></data>
						</cell>
						<cell name="bin_type" col="6" format="text" datatype="string" length="10" pk="0">
							<data type="source" table="bin" field="bin_type" value="value"></data>
						</cell>
						<cell name="bin_area" col="7" format="text" datatype="string" length="10" pk="0">
							<data type="source" table="bin" field="bin_area" value="value"></data>
						</cell>
						<cell name="area" col="8" format="text" datatype="integer" length="12" pk="0">
							<data type="source" table="bin" field="area" value="value"></data>
						</cell>
						<cell name="palls" col="9" format="text" datatype="integer" length="12" pk="0">
							<data type="source" table="bin" field="palls" value="value"></data>
						</cell>
						<cell name="weight" col="10" format="text" datatype="integer" length="12" pk="0">
							<data type="source" table="bin" field="weight" value="value"></data>
						</cell>
						<cell name="volume" col="11" format="text" datatype="integer" length="20" pk="0">
							<data type="source" table="bin" field="volume" value="value"></data>
						</cell>
						<cell name="canpick" col="12" format="text" datatype="integer" length="1" pk="0">
							<data type="source" table="bin" field="canpick" value="value"></data>
						</cell>
						<cell name="isblocked" col="13" format="text" datatype="integer" length="1" pk="0">
							<data type="source" table="bin" field="isblocked" value="value"></data>
						</cell>
						<cell name="define1" col="14" format="text" datatype="string" length="50" pk="0">
							<data type="source" table="bin" field="define1" value="value"></data>
						</cell>
						<cell name="define2" col="15" format="text" datatype="string" length="50" pk="0">
							<data type="source" table="bin" field="define2" value="value"></data>
						</cell>
						<cell name="define3" col="16" format="text" datatype="string" length="50" pk="0">
							<data type="source" table="bin" field="define3" value="value"></data>
						</cell>
						<cell name="define4" col="17" format="text" datatype="string" length="50" pk="0">
							<data type="source" table="bin" field="define4" value="value"></data>
						</cell>
						<cell name="define5" col="18" format="text" datatype="string" length="50" pk="0">
							<data type="source" table="bin" field="define5" value="value"></data>
						</cell>
						<cell name="opt_by" col="19" format="text" datatype="string" length="20" pk="0">
							<data type="source" table="bin" field="opt_by" value="value"></data>
						</cell>
						-->
				</body>
				<body type="footer">
					<!--not supported--></body>
			</table>
			<table type="filter" columns="3"></table>
		</target>
	</process>
</define>