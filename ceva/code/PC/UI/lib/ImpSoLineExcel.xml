<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.2">
	<process type="import">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
		<source type="xlsfile">
			<!--
			note: all word please use lower-case.
			type="businessobject",dataset,txtfile,xlsfile,database
			-->
			<table type="column" name="pop" coltype="name" firstrow="2">
				<!--
				columntype=name, position(column must have col property)
				firstdata=1(only for text source)
				delimiter=tab,comma(only for text file), or none(fix length)
				sql= sql statement (only for database)
				-->

				<field name="STCD" caption="公司ID" datatype="string" position="1" length="6" pk="1" />
				<field name="ORDNUMBER" caption="SO号" datatype="string" position="2" length="22" pk="1" />
				<field name="LINENUM" caption="明细行号" datatype="integer" position="3" length="50" pk="1" />
				<field name="ITEM" caption="物料编码" datatype="string" position="4" length="24" pk="0" />
				<field name="ITEM_DESC" caption="物料名称" datatype="string" position="5" length="60" pk="0" />
				<field name="QTYORDERED" caption="订单数量" datatype="integer" position="6" length="100" pk="0" />
				
			</table>
		</source>
		<!--
	type=xlsfile,txtfile,databases
	method="excel",html,xml
	template's file must store same with this file.
	 -->
		<target seq="1" type="database">
			<table name="iSO_LIN" type="data">
				<body type="header"></body>
				<body type="body" firstrow="2" maxrecords="-1" sql="stp_imp_SoLine">
					<paramerters>
						<field name="@seq_id" caption="seq id" datatype="long" length="20" pk="1" value="">
							<data type="runtime" field="seq_id" value="value"></data>
						</field>
					</paramerters>
					<!--rows will repeat output-->
					<cell name="seq_id" col="1" format="text" datatype="long" length="20" pk="1">
						<data type="runtime" field="seq_id" value="value"></data>
					</cell>
					<cell name="STCD" col="2" format="text" datatype="string" length="6" pk="1">
						<data type="source" table="pop" field="STCD" value="value"></data>
					</cell>
					<cell name="ORDNUMBER" col="3" format="text" datatype="string" length="22" pk="1">
						<data type="source" table="pop" field="ORDNUMBER" value="value"></data>
					</cell>
					<cell name="LINENUM" col="4" format="text" datatype="integer" length="50" pk="1">
						<data type="source" table="pop" field="LINENUM" value="value"></data>
					</cell>
					<cell name="ITEM" col="5" format="text" datatype="string" length="24" pk="0">
						<data type="source" table="pop" field="ITEM" value="value"></data>
					</cell>
					<cell name="ITEM_DESC" col="6" format="text" datatype="string" length="60" pk="0">
						<data type="source" table="pop" field="ITEM_DESC" value="value"></data>
					</cell>
					<cell name="QTYORDERED" col="7" format="text" datatype="integer" length="60" pk="0">
						<data type="source" table="pop" field="QTYORDERED" value="value"></data>
					</cell>
				</body>
				<body type="footer">
					<!--not supported--></body>
			</table>
			<table type="filter" columns="3"></table>
		</target>
	</process>
</define>