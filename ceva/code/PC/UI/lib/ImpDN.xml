<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.2">
	<process type="import">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
		<source type="txtfile">
			<!--
			note: all word please use lower-case.
			type="businessobject",dataset,txtfile,xlsfile,database
			-->
			<table type="column" name="DN" coltype="name" firstrow="2" delimiter="comma">
				<!--
				columntype=name, position(column must have col property)
				firstdata=1(only for text source)
				delimiter=tab,comma(only for text file), or none(fix length)
				sql= sql statement (only for database)
				-->

				<field name="due_date" caption="出货日期" datatype="date" position="1" length="20" pk="0" />
				<field name="dn_no" caption="单号" datatype="string" position="4" length="20" pk="1" />
				<field name="deal_to" caption="收货方编号" datatype="string" position="10" length="10" pk="0" />
				<field name="deal_name" caption="收货方名称" datatype="string" position="11" length="50" pk="0" />
				<field name="deal_person" caption="收货方联系人" datatype="string" position="13" length="50" pk="0" />
				<field name="deal_tel" caption="收货方联系电话" datatype="string" position="14" length="30" pk="0" />
				<field name="unloading_address" caption="收货方地址" datatype="string" position="12" length="100" pk="0" />
				<field name="city_to" caption="目的城市" datatype="string" position="15" length="10" pk="0" />
				<field name="condition" caption="运输方式" datatype="string" position="16" length="50" pk="0" />
				<field name="method" caption="配送方式" datatype="string" position="17" length="50" pk="0" />
				<field name="sku_no" caption="产品编码" datatype="string" position="18" length="20" pk="0" />
				<field name="qty" caption="发货数量" datatype="integer" position="24" length="12" pk="0" />
				
			</table>
		</source>
		<!--
	type=xlsfile,txtfile,databases
	method="excel",html,xml
	template's file must store same with this file.
	 -->
		<target seq="1" type="database">
			<table name="ImpDn" type="data">
				<body type="header"></body>
				<body type="body" firstrow="2" maxrecords="-1" sql="stp_imp_DN">
					<paramerters>
						<field name="@id" caption="id" datatype="long" length="20" pk="1" value="">
							<data type="runtime" field="id" value="value"></data>
						</field>
					</paramerters>
					<!--rows will repeat output-->
					<cell name="id" col="1" format="text" datatype="long" length="20" pk="1">
						<data type="runtime" field="id" value="value"></data>
					</cell>
					<cell name="dc_code" col="2" format="text" datatype="string" length="8" pk="1">
						<data type="value" value="SHA"></data>
					</cell>
					<cell name="due_date" col="3" format="text" datatype="string" length="" pk="0">
						<data type="source" table="DN" field="due_date" value="value"></data>
					</cell>
					<cell name="dn_no" col="4" format="text" datatype="string" length="20" pk="1">
						<data type="source" table="DN" field="dn_no" value="value"></data>
					</cell>
					<cell name="deal_to" col="5" format="text" datatype="string" length="10" pk="0">
						<data type="source" table="DN" field="deal_to" value="value"></data>
					</cell>
					<cell name="deal_name" col="6" format="text" datatype="string" length="50" pk="0">
						<data type="source" table="DN" field="deal_name" value="value"></data>
					</cell>
					<cell name="deal_person" col="7" format="text" datatype="string" length="50" pk="0">
						<data type="source" table="DN" field="deal_person" value="value"></data>
					</cell>
					<cell name="deal_tel" col="8" format="text" datatype="string" length="30" pk="0">
						<data type="source" table="DN" field="deal_tel" value="value"></data>
					</cell>
					<cell name="unloading_address" col="9" format="text" datatype="string" length="100" pk="0">
						<data type="source" table="DN" field="unloading_address" value="value"></data>
					</cell>
					<cell name="city_to" col="10" format="text" datatype="string" length="10" pk="0">
						<data type="source" table="DN" field="city_to" value="value"></data>
					</cell>
					<cell name="condition" col="11" format="text" datatype="string" length="50" pk="0">
						<data type="source" table="DN" field="condition" value="value"></data>
					</cell>
					<cell name="method" col="12" format="text" datatype="string" length="50" pk="0">
						<data type="source" table="DN" field="method" value="value"></data>
					</cell>
					<cell name="sku_no" col="13" format="text" datatype="string" length="20" pk="1">
						<data type="source" table="DN" field="sku_no" value="value"></data>
					</cell>
					<cell name="qty" col="14" format="text" datatype="integer" length="12" pk="0">
						<data type="source" table="DN" field="qty" value="value"></data>
					</cell>
					<cell name="sony_bch_no" col="15" format="text" datatype="string" length="20" pk="0">
						<data type="runtime" field="sony_bch_no" value="value"></data>
					</cell>
					<cell name="doc_type" col="16" format="text" datatype="string" length="3" pk="0">
						<data type="runtime" field="doc_type" value="value"></data>
					</cell>
					<cell name="opt_by" col="17" format="text" datatype="string" length="20" pk="0">
						<data type="runtime" field="opt_by" value="value"></data>
					</cell>
				</body>
				<body type="footer">
					<!--not supported--></body>
			</table>
			<table type="filter" columns="3"></table>
		</target>
	</process>
</define>