<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.0">
	<process type="export">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
	<source type="DataSet">
		<!--
			note: all word please use lower-case.
			type="businessobject",DataSet,textfile,xlsfile
			object_type: no-use
			columntype=name, position(column must have col property)
			firstdata=1(only for text source)
			delimiter=tab,comma(only for text file)
			-->								

		<table type="column" name="DNList" coltype="name">
			<field name="opt_dtime" caption="操作日期" datatype="string" length="20" pk="0" />
			<field name="dn_no" caption="单号" datatype="string" length="20" pk="0" />
			<field name="sony_bch_no" caption="索尼批次" datatype="string" length="20" pk="0" />
			<field name="bch_no" caption="CEVA批次" datatype="string" length="20" pk="0" />
			<field name="city_to" caption="送达城市" datatype="string" length="20" pk="0" />
			<field name="sku_no" caption="物料" datatype="string" length="20" pk="0" />
			<field name="model_no" caption="型号" datatype="string" length="20" pk="0" />
			<field name="qty" caption="数量" datatype="string" length="20" pk="0" />
			<field name="bin_code" caption="库位" datatype="string" length="20" pk="0" />
		</table>
	</source>
	<!--
	type=xlsfile,textfile
	method="Excel",html,xml
	template's file must store same with this file.
	 -->
	<target seq="1" type="xlsfile" method="Excel">
		<table name="机身号" columns="3" type="data">			
			<body type="header" firstrow="1">
				<cell name="opt_dtime" col="1" format="text">
					<data type="value" value="操作日期"></data>
				</cell>
				<cell name="dn_no" col="2" format="text">
					<data type="value" value="单号"></data>
				</cell>
				<cell name="sony_bch_no" col="3" format="text">
					<data type="value" value="索尼批次"></data>
				</cell>
				<cell name="bch_no" col="4" format="text">
					<data type="value" value="CEVA批次"></data>
				</cell>
				<cell name="city_to" col="5" format="text">
					<data type="value" value="送达城市"></data>
				</cell>
				<cell name="sku_no" col="6" format="text">
					<data type="value" value="物料"></data>
				</cell>
				<cell name="model_no" col="7" format="text">
					<data type="value" value="型号"></data>
				</cell>
				<cell name="qty" col="8" format="text">
					<data type="value" value="数量"></data>
				</cell>
				<cell name="bin_code" col="9" format="text">
					<data type="value" value="库位"></data>
				</cell>
			</body>
			<body type="body" firstrow="2" maxrecords="-1">
				<!--Rows will repeat output-->
				<cell name="opt_dtime" col="1" format="text">
					<data type="source" table="DNList" field="opt_dtime" value="value"></data>
				</cell>
				<cell name="dn_no" col="2" format="text">
					<data type="source" table="DNList" field="dn_no" value="value"></data>
				</cell>
				<cell name="sony_bch_no" col="3" format="text">
					<data type="source" table="DNList" field="sony_bch_no" value="value"></data>
				</cell>
				<cell name="bch_no" col="4" format="text">
					<data type="source" table="DNList" field="bch_no" value="value"></data>
				</cell>
				<cell name="city_to" col="5" format="text">
					<data type="source" table="DNList" field="city_to" value="value"></data>
				</cell>
				<cell name="sku_no" col="6" format="text">
					<data type="source" table="DNList" field="sku_no" value="value"></data>
				</cell>
				<cell name="model_no" col="7" format="text">
					<data type="source" table="DNList" field="model_no" value="value"></data>
				</cell>
				<cell name="qty" col="8" format="text">
					<data type="source" table="DNList" field="qty" value="value"></data>
				</cell>
				<cell name="bin_code" col="9" format="text">
					<data type="source" table="DNList" field="bin_code" value="value"></data>
				</cell>
			</body>
			<body type="footer"></body>
		</table>
	</target>
	</process>
</define>