<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.0">
	<source type="businessobject">
		<!--
			note: all word please use lower-case.
			type="businessobject",DataSet,textfile,xlsfile
			object_type: no-use
			columntype=name, position(column must have col property)
			firstdata=1(only for text source)
			delimiter=tab,comma(only for text file)
			-->
		<table type="column" name="inoutstore" coltype="name">
			<!--month,dc_name,own_name,wh_name,sku_no,model_no,category,cost_type,sku_status,description,out_num,in_num,store_qty,pre_store,post_store,turn_num,store_days-->
			<field name="公司ID" caption="公司ID" datatype="string" length="6" pk="0" />
			<field name="采购单号" caption="采购单号" datatype="string" length="50" pk="0" />
			<field name="采购明细行" caption="采购明细行" datatype="string" length="50" pk="0" />
			<field name="物料编码" caption="物料编码" datatype="string" length="50" pk="0" />
			<field name="物料名称" caption="物料名称" datatype="string" length="60" pk="0" />
			<field name="批次号" caption="批次号" datatype="string" length="50" pk="0" />
			<field name="入库数量" caption="入库数量" datatype="string" length="50" pk="0" />
			<field name="入库人" caption="入库人" datatype="string" length="50" pk="0" />
			<field name="入库时间" caption="入库时间" datatype="date" length="6" pk="0" />
		</table>
	</source>
	<!--
	type=xlsfile,textfile
	method="Excel",html,xml
	template's file must store same with this file.
	 -->
	<target seq="1" type="xlsfile" method="Excel" template="Template\InOutStoreGather.xls">
		<table name="产品采购汇总报表" columns="9" type="data">			
			<body type="body" firstrow="2" maxrecords="-1">
				<!--Rows will repeat output-->
				<cell name="公司ID" col="1" format="text">
					<data type="source" table="inoutstore" field="公司ID" value="value"></data>
				</cell>
				<cell name="采购单号" col="2" format="text">
					<data type="source" table="inoutstore" field="采购单号" value="value"></data>
				</cell>
				<cell name="采购明细行" col="3" format="text">
					<data type="source" table="inoutstore" field="采购明细行" value="value"></data>
				</cell>
				<cell name="物料编码" col="4"  format="text">
					<data type="source" table="inoutstore" field="物料编码" value="value"></data>
				</cell>
				<cell name="物料名称" col="5" format="text">
					<data type="source" table="inoutstore" field="物料名称" value="value"></data>
				</cell>
				<cell name="批次号" col="6" format="text">
					<data type="source" table="inoutstore" field="批次号" value="value"></data>
				</cell>
				<cell name="入库数量" col="7" format="text">
					<data type="source" table="inoutstore" field="入库数量" value="value"></data>
				</cell>
				<cell name="入库人" col="8"  format="text">
					<data type="source" table="inoutstore" field="入库人" value="value"></data>
				</cell>
				<cell name="入库时间" col="9"  format="text">
					<data type="source" table="inoutstore" field="入库时间" value="value"></data>
				</cell>
			</body>
			<body type="footer"></body>
		</table>
	</target>
</define>