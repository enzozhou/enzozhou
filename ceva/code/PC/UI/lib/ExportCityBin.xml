<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.0">
	<process type="export">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
	<source type="DataSet">
		<!--
			note: all word please use lower-case.
			type="businessobject",DataSet,textfile,xlsfile
			object_type: no-use
			columntype=name, position(column must have col property)
			firstdata=1(only for text source)
			delimiter=tab,comma(only for text file)
			-->
		<table type="column" name="DN" coltype="name">
			<field name="city_to" caption="站点" datatype="string" length="20" pk="0" />
			<field name="bin_code" caption="库位" datatype="string" length="20" pk="0" />
		</table>
	</source>
	<!--
	type=xlsfile,textfile
	method="Excel",html,xml
	template's file must store same with this file.
	 -->
	<target seq="1" type="xlsfile" method="Excel">
		<table name="站点" columns="3" type="data">
			<body type="header" firstrow="1">
				<cell name="city_to" col="1" format="text">
					<data type="value" value="站点"></data>
				</cell>
				<cell name="bin_code" col="2" format="text">
					<data type="value" value="库位"></data>
				</cell>
			</body>
			<body type="body" firstrow="2" maxrecords="-1">
				<!--Rows will repeat output-->
				<cell name="city_to" col="1" format="text">
					<data type="source" table="DN" field="city_to" value="value"></data>
				</cell>
				<cell name="bin_code" col="2" format="text">
					<data type="source" table="DN" field="bin_code" value="value"></data>
				</cell>
			</body>
			<body type="footer"></body>
		</table>
	</target>
	</process>
</define>