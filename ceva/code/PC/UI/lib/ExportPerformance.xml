<?xml version="1.0" encoding="utf-8" ?> 
<define version="1.0.0.0" requireversion="1.0.0.0">
	<process type="export">
		<!--
	type=export(runexport),import(runimport),convert(runconvert)
	not used
	-->
	<source type="DataSet">
		<!--
			note: all word please use lower-case.
			type="businessobject",DataSet,textfile,xlsfile
			object_type: no-use
			columntype=name, position(column must have col property)
			firstdata=1(only for text source)
			delimiter=tab,comma(only for text file)
			-->
		<table type="column" name="Performance" coltype="name">
			<field name="opt_dtime" caption="操作日期" datatype="string" length="20" pk="0" />
			<field name="opt_by" caption="操作人员" datatype="string" length="20" pk="0" />
			<field name="sony_bch_no" caption="索尼批次" datatype="string" length="20" pk="0" />
			<field name="bch_no" caption="CEVA批次" datatype="string" length="20" pk="0" />
			<field name="sum_qty" caption="操作件数" datatype="string" length="20" pk="0" />
			<field name="sum_dn" caption="操作票数" datatype="string" length="20" pk="0" />
		</table>
	</source>
	<!--
	type=xlsfile,textfile
	method="Excel",html,xml
	template's file must store same with this file.
	 -->
	<target seq="1" type="xlsfile" method="Excel">
		<table name="人员绩效统计" columns="3" type="data">			
			<body type="header" firstrow="1">
				<cell name="opt_dtime" col="1" format="text">
					<data type="value" value="操作日期"></data>
				</cell>
				<cell name="opt_by" col="2" format="text">
					<data type="value" value="操作人员"></data>
				</cell>
				<cell name="sony_bch_no" col="3" format="text">
					<data type="value" value="索尼批次"></data>
				</cell>
				<cell name="bch_no" col="4" format="text">
					<data type="value" value="CEVA批次"></data>
				</cell>
				<cell name="sum_qty" col="5" format="text">
					<data type="value" value="操作件数"></data>
				</cell>
				<cell name="sum_dn" col="6" format="text">
					<data type="value" value="操作票数"></data>
				</cell>
			</body>
			<body type="body" firstrow="2" maxrecords="-1">
				<!--Rows will repeat output-->
				<cell name="opt_dtime" col="1" format="text">
					<data type="source" table="Performance" field="opt_dtime" value="value"></data>
				</cell>
				<cell name="opt_by" col="2" format="text">
					<data type="source" table="Performance" field="opt_by" value="value"></data>
				</cell>
				<cell name="sony_bch_no" col="3" format="text">
					<data type="source" table="Performance" field="sony_bch_no" value="value"></data>
				</cell>
				<cell name="bch_no" col="4" format="text">
					<data type="source" table="Performance" field="bch_no" value="value"></data>
				</cell>
				<cell name="sum_qty" col="5" format="text">
					<data type="source" table="Performance" field="sum_qty" value="value"></data>
				</cell>
				<cell name="sum_dn" col="6" format="text">
					<data type="source" table="Performance" field="sum_dn" value="value"></data>
				</cell>
			</body>
			<body type="footer"></body>
		</table>
	</target>
	</process>
</define>